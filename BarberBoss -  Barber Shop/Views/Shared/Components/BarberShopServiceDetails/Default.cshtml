@model BarberBoss___Barber_Shop.ViewModels.BarberShopsServices.BarberShopsServiceDetailsViewModel
@{
    ViewData["Title"] = "Default";
}

<div class="form-group row">
    <label class="col-sm-2 col-form-label">BarberShop</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.BarberShopName</div>
    </div>
</div>
<div class="form-group row">
    <label class="col-sm-2 col-form-label">Address</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.BarberShopTownName, @Model.BarberShopAddress</div>
    </div>
</div>
<div class="form-group row">
    <label class="col-sm-2 col-form-label">Service</label>
    <div class="col-sm-10">
        <div class="form-control">@Model.ServiceName</div>
    </div>
</div>
